#include <stdio.h>

int main(){

    int value; 
    int FT = 0, ST = 1, NT;

    printf("Enter Your number");
    scanf("%d", &value);

    printf("Fibbnacii series till your number is = ");

    for(int i = 0; i<=value; i++){

        NT = FT + ST;
        FT = ST;
        ST = NT;

        printf("%d", FT);

    }
return 0;
}